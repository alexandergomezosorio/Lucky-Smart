<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo4"
    tools:context=".RegisterSellerActivity">

    <ImageButton
        android:id="@+id/btnBack4"
        android:layout_width="66dp"
        android:layout_height="58dp"
        android:background="@color/white"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.975"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etCity2"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/outline_arrow_back_ios_black_36"
        app:tint="@color/black"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/btnSignSeller"
        android:layout_width="173dp"
        android:layout_height="55dp"
        android:layout_marginStart="72dp"
        android:backgroundTint="@color/primary"
        android:text="@string/next"
        android:textColor="@color/black"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.25"
        app:strokeColor="@color/black"
        app:strokeWidth="2dp" />

    <EditText
        android:id="@+id/etCity2"
        android:layout_width="385dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/style_border_editext"
        android:drawableLeft="@drawable/outline_location_on_black_18"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="@string/city"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.716"
        app:layout_constraintStart_toEndOf="@+id/btnSignSeller"
        app:layout_constraintTop_toBottomOf="@+id/etName4" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="260dp"
        android:layout_height="55dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/amaranth_bold_italic"
        android:text="@string/registerSeller"
        android:textColor="@color/black"
        android:textSize="28sp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginTop="68dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:srcCompat="@drawable/logo1" />

    <EditText
        android:id="@+id/etEmail"
        android:layout_width="385dp"
        android:layout_height="48dp"
        android:layout_marginTop="76dp"
        android:background="@drawable/style_border_editext"
        android:drawableLeft="@drawable/outline_email_black_18"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="@string/email"
        android:inputType="textEmailAddress"
        android:minHeight="48dp"
        android:textColor="@color/black"
        android:visibility="visible"
        app:cornerRadius="200dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.913"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etPassword"
        android:layout_width="385dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/style_border_editext"
        android:drawableLeft="@drawable/outline_password_black_18"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="@string/password"
        android:inputType="textPassword"
        android:minHeight="48dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.756"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/etEmail" />

    <EditText
        android:id="@+id/etName4"
        android:layout_width="385dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/style_border_editext"
        android:drawableLeft="@drawable/outline_location_on_black_18"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="@string/country"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.913"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etPassword" />

</androidx.constraintlayout.widget.ConstraintLayout>